<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.vpool.model.EmergencyConnectModel" table = "emergencyconnect">
      
      <meta attribute = "class-description">
         This class contains the emergency contact details. 
      </meta>
      
      <id name = "emgID" type = "integer" column="emgID">
      	<generator class="increment"></generator>
      </id>
      
      <property name = "emgName" column = "emgName" type = "string" not-null="true" length="50"/>
      
      <property name = "emgContact" column = "emgContact" type = "string" not-null="true" length="50"/>
   </class>
</hibernate-mapping>