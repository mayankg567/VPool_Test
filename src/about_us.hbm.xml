<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.vpool.model.AboutUsModel" table = "aboutus">
      
      <meta attribute = "class-description">
         This class contains the company details. 
      </meta>
      
      <id name = "mobNo" type = "string" column="mobNo" length="20">
      </id>
      
      <property name = "tagLine" column = "tagLine" type = "string" not-null="true" length="256"/>
      
      <property name = "description" column = "description" type = "string" not-null="true" length="1000"/>
      
      <property name = "address" column = "address" type = "string" not-null="true" length="1000"/>
      
      <property name = "mission" column = "mission" type = "string" not-null="true" length="1000"/>
      
      <property name = "vision" column = "vision" type = "string" not-null="true" length="1000"/>
      
      <property name = "altMobNo" column = "altMobNo" type = "string" not-null="true" length="20"/>
      
      <property name = "telephone" column = "telephone" type = "string" not-null="true" length="20"/>
   </class>
</hibernate-mapping>